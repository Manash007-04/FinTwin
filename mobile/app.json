{
  "expo": {
    "name": "FinTwin",
    "slug": "fintwin-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "fintwin",
    "userInterfaceStyle": "light",
    "splash": {
      "backgroundColor": "#F8FAF5"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fintwin.mobile"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#2D3C59"
      },
      "package": "com.fintwin.mobile"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}